{
  "name": "@1stcathays/remix-logging",
  "version": "0.1.0",
  "description": "Logging extensions for Remix applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run esbuild && npm run types",
    "clean": "rm -rf dist",
    "esbuild": "../../build.js",
    "types": "tsc --project tsconfig.types.json --emitDeclarationOnly --outDir dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/1st-Cathays/remix-packages/packages/logging"
  },
  "keywords": [
    "remix",
    "react",
    "pino",
    "logger"
  ],
  "author": "Ahmed Hussein",
  "dependencies": {
    "pino": "^9.3.1"
  }
}
